{
  "shell": "bash",
  "yabaiPath": "/opt/homebrew/bin/yabai",
  "slidingAnimationPace": 8,
  "themeScheme": "dark",
  "theme": {
    "dark": {
      "main": "#0f0f0f",
      "mainAlt": "#464646",
      "minor": "#2b2b2b",
      "red": "#e78482",
      "green": "#8fc8BB",
      "blue": "#6db3cE",
      "yellow": "#ffd484",
      "orange": "#ffb374",
      "magenta": "#ad82cB",
      "cyan": "#7eddde",
      "foreground": "#f5f5f5",
      "gradient": "linear-gradient(0.45turn, var(--spl-cyan), var(--spl-magenta))"
    },
    "black": "#0f0f0f",
    "white": "#f5f5f5",
    "barWidth": "100vw",
    "barHeight": "24px",
    "barVerticalOffset": "0px",
    "barHorizontalOffset": "0px",
    "barInnerMargin": "3px",
    "barRadius": "0px",
    "barOpacity": "0.8",
    "spaceMargin": "0px 12px 0px 0px",
    "processWidth": "100%",
    "itemRadius": "18px",
    "itemMargin": "0px 0px 0px 4px",
    "itemInnerMargin": "3px 7px",
    "fontFamily": "JetBrains Mono, monospace",
    "fontSize": "12px",
    "shadow": "0 5px 10px rgba(0, 0, 0, 0.24)",
    "transitionEasing": "cubic-bezier(0.4, 0, 0.2, 1)"
  }
}